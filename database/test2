CREATE DATABASE IF NOT EXISTS mahasiswa;
USE mahasiswa;

DROP TABLE IF EXISTS mahasiswa;
CREATE TABLE mahasiswa (
    npm VARCHAR(20) PRIMARY KEY,
    nama VARCHAR(100),
    semester INT,
    angkatan INT,
    ipk DECIMAL(4,2),
    nilai INT,
    matkul VARCHAR(100),
    kehadiran INT,
    prestasi INT,
    organisasi INT
);

